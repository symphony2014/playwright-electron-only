    "playwright-core": "^1.56.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ssim.js": "^3.5.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "ws": "^8.18.3",
    "xml2js": "^0.5.0",
    "yaml": "^2.6.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "dependencies": {
    "commander": "^14.0.1",
